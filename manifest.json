{
  "manifest_version": 2,

  "name": "Face Lock",
  "description": "We make faces smile",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [{
      "matches": ["http://www.google.com/*"],
      "js": ["js/check_form.js"],
      "run_at": "document_end"
  }],
  "background": {
    "scripts": ["js/jquery-3.1.1.min.js", "js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enter Info"
  }
}
